{
  "tasks": [
    {
      "id": "topological-sort",
      "description": "Sort dependencies so each item comes after its dependencies",
      "examples": [
        {
          "input": [{"name":"c","deps":["a","b"]},{"name":"a","deps":[]},{"name":"b","deps":["a"]}],
          "expected_output": ["a","b","c"]
        }
      ]
    },
    {
      "id": "json-diff",
      "description": "Find differences between two objects, return only changed fields",
      "examples": [
        {
          "input": {"old": {"a":1,"b":2,"c":3}, "new": {"a":1,"b":5,"d":4}},
          "expected_output": {"changed":{"b":{"from":2,"to":5}},"added":{"d":4},"removed":{"c":3}}
        }
      ]
    },
    {
      "id": "balanced-brackets",
      "description": "Check if string has balanced brackets, return depth",
      "examples": [
        {"input": "((()))", "expected_output": {"balanced": true, "max_depth": 3}},
        {"input": "(()", "expected_output": {"balanced": false, "max_depth": 2}},
        {"input": "(()())", "expected_output": {"balanced": true, "max_depth": 2}}
      ]
    },
    {
      "id": "longest-increasing",
      "description": "Find longest increasing subsequence",
      "examples": [
        {"input": [10,22,9,33,21,50,41,60], "expected_output": [10,22,33,50,60]},
        {"input": [3,1,2,4], "expected_output": [1,2,4]}
      ]
    },
    {
      "id": "tree-to-paths",
      "description": "Convert nested tree to all root-to-leaf paths with accumulated values",
      "examples": [
        {
          "input": {"val":1,"children":[{"val":2,"children":[{"val":4,"children":[]}]},{"val":3,"children":[]}]},
          "expected_output": [[1,2,4],[1,3]]
        }
      ]
    },
    {
      "id": "moving-average",
      "description": "Calculate moving average with window size 3",
      "examples": [
        {"input": [1,2,3,4,5,6], "expected_output": [2,3,4,5]},
        {"input": [10,20,30,40], "expected_output": [20,30]}
      ]
    },
    {
      "id": "regex-match-extract",
      "description": "Extract all numbers from strings in array",
      "examples": [
        {"input": ["abc123def","456","no numbers","7x8x9"], "expected_output": [[123],[456],[],[7,8,9]]}
      ]
    }
  ]
}
