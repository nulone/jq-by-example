{
  "tasks": [
    {
      "id": "tree-depth",
      "description": "Calculate max depth of nested object",
      "examples": [
        {"input": {"a": {"b": {"c": 1}}}, "expected_output": 3},
        {"input": {"a": 1, "b": {"c": 2}}, "expected_output": 2},
        {"input": {"x": 1}, "expected_output": 1}
      ]
    },
    {
      "id": "running-total",
      "description": "Calculate running total for array of numbers",
      "examples": [
        {"input": [1, 2, 3, 4], "expected_output": [1, 3, 6, 10]},
        {"input": [5, 5, 5], "expected_output": [5, 10, 15]}
      ]
    },
    {
      "id": "group-and-aggregate",
      "description": "Group by category and calculate sum and count",
      "examples": [
        {
          "input": [{"cat": "a", "val": 10}, {"cat": "b", "val": 20}, {"cat": "a", "val": 5}],
          "expected_output": [{"cat": "a", "sum": 15, "count": 2}, {"cat": "b", "sum": 20, "count": 1}]
        }
      ]
    },
    {
      "id": "diff-consecutive",
      "description": "Calculate difference between consecutive elements",
      "examples": [
        {"input": [10, 15, 12, 20], "expected_output": [5, -3, 8]},
        {"input": [1, 1, 1], "expected_output": [0, 0]}
      ]
    },
    {
      "id": "invert-object",
      "description": "Swap keys and values, handling duplicate values by making arrays",
      "examples": [
        {"input": {"a": 1, "b": 2, "c": 1}, "expected_output": {"1": ["a", "c"], "2": ["b"]}},
        {"input": {"x": "foo", "y": "bar"}, "expected_output": {"foo": ["x"], "bar": ["y"]}}
      ]
    },
    {
      "id": "matrix-transpose",
      "description": "Transpose a matrix (2D array)",
      "examples": [
        {"input": [[1,2,3],[4,5,6]], "expected_output": [[1,4],[2,5],[3,6]]},
        {"input": [[1,2],[3,4],[5,6]], "expected_output": [[1,3,5],[2,4,6]]}
      ]
    },
    {
      "id": "path-to-value",
      "description": "Find all paths to leaf values in nested object",
      "examples": [
        {"input": {"a": {"b": 1}, "c": 2}, "expected_output": [["a","b",1], ["c",2]]}
      ]
    }
  ]
}
